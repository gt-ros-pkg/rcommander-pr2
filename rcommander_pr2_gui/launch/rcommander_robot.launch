<launch>
  <!--
  <include file="$(find rcommander)/launch/c_jt.launch"/>
  -->
  <!--
  <include file="$(find sound_play)/soundplay_node.launch"/>
  -->

  <include file="$(find pr2_interactive_manipulation)/launch/pr2_interactive_manipulation_robot.launch"/>
  <include file="$(find pr2_gripper_sensor_action)/launch/pr2_gripper_sensor_actions.launch"/>
  <include file="$(find tts_server)/tts_server.launch"/>
  <node pkg="pr2_tuckarm" type="tuck_arms.py" output="screen" name="tuck_arms"/>

  <node pkg="ptp_arm_action" type="ptp_arm_action_server.py" args="left" output="screen" name="ptp_action_left">
  	<param name="translation_tolerance" value=".015"/>	
        <param name="rotation_tolerance" value="5"/>
        <param name="timeout" value="30"/>
        <param name="stall_time" value="3."/>
  </node>

  <node pkg="ptp_arm_action" type="ptp_arm_action_server.py" args="right" output="screen" name="ptp_action_right">
  	<param name="translation_tolerance" value=".015"/>	
        <param name="rotation_tolerance" value="5"/>
        <param name="timeout" value="30"/>
        <param name="stall_time" value="3."/>
  </node>

</launch>
