<package>
  <description brief="rcommander_pr2_gui">

     rcommander_pr2

  </description>
  <author>Hai Nguyen</author>
  <license>BSD</license>
  <review status="unreviewed" notes=""/>
  <url>http://ros.org/wiki/rcommander_pr2</url>
  <depend package="rcommander_core"/>
  <depend package="tts_server"/>
  <depend package="pr2_common_action_msgs"/>
  <depend package="pr2_controllers_msgs"/>
  <depend package="geometry_msgs"/>
  <depend package="trajectory_msgs"/>
  <depend package="smach"/>
  <depend package="rospy"/>
  <depend package="tf"/>
  <depend package="object_manipulator"/>
  <depend package="ptp_arm_action"/>
  <depend package="arm_navigation_msgs"/>
  <depend package="actionlib"/>
  <depend package="move_base_msgs"/>
  <depend package="pr2_gripper_sensor_msgs"/>
  <depend package="sound_play"/>
  <depend package="sensor_msgs"/>
  <depend package="std_msgs"/>
  <depend package="actionlib"/>

  <export>
      <rcommander plugin="rcommander_pr2.tuck_tool"          robot="pr2" tab="Manipulation"/> 
      <rcommander plugin="rcommander_pr2.linear_move_tool"   robot="pr2" tab="Manipulation"/>
      <rcommander plugin="rcommander_pr2.move_arm_tool"      robot="pr2" tab="Manipulation"/>
      <rcommander plugin="rcommander_pr2.move_tool"          robot="pr2" tab="Manipulation"/>
      <rcommander plugin="rcommander_pr2.gripper_tool"       robot="pr2" tab="Manipulation"/>
      <rcommander plugin="rcommander_pr2.gripper_event_tool" robot="pr2" tab="Manipulation"/>
      <rcommander plugin="rcommander_pr2.spine_tool"         robot="pr2" tab="Manipulation"/> 
      <rcommander plugin="rcommander_pr2.navigate_tool"      robot="all" tab="Navigation"/>      
      <rcommander plugin="rcommander_pr2.speak_tool"         robot="all" tab="Interaction"/>  
      <rcommander plugin="rcommander_pr2.move_head_tool"     robot="pr2" tab="Interaction"/>  
  </export>

</package>


