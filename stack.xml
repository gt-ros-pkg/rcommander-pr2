  <depend stack="arm_navigation" /> <!-- arm_navigation_msgs -->
  <depend stack="executive_smach" /> <!-- smach_ros, smach -->
  <depend stack="pr2_controllers" /> <!-- pr2_controllers_msgs -->
  <depend stack="pr2_mechanism" /> <!-- pr2_mechanism_msgs -->
</stack>
