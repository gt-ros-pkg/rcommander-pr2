  <url>http://ros.org/wiki/rcommander</url>

  <depend stack="ros" />
  <depend stack="ros_comm" /> <!-- std_msgs, rospy -->
  <depend stack="common" /> <!-- actionlib -->
  <depend stack="common_msgs" /> <!-- sensor_msgs, trajectory_msgs, geometry_msgs -->

  <depend stack="geometry" /> <!-- tf -->
